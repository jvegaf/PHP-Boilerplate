{
  "autoload": {
    "psr-4": {
      "ApplicationName\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationName\\Test\\": "test/"
    }
  },
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8"
  },
  "scripts": {
    "test": "phpunit --configuration phpunit.xml"
  }
}
